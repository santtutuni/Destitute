{
  "name": "software-testing-assignment",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "test": "jest --coverage",
    "coverage": "jest --coverage",
    "coveralls": "jest --coverage && cat ./coverage/lcov.info | coveralls"
  },
  "jest": {
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/tests"
    ],
    "collectCoverage": true,
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "text",
      "lcov"
    ],
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "extensionsToTreatAsEsm": [
      ".ts",
      ".jsx",
      ".js",
      ".mjs"
    ]
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/preset-env": "^7.26.0",
    "babel-jest": "^29.7.0",
    "coveralls": "^3.1.0",
    "coveralls-next": "^4.2.1",
    "jest": "^29.7.0",
    "jest-junit": "^12.0.0"
  }
}
